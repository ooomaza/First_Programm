meme_dict = {
            "КРИНЖ": "Что-то очень странное или стыдное",
            "ЛОЛ": "Что-то очень смешное",
            "ЮЗАТЬ": 'От Английского слова use-использовать',
            "ИМБА": 'очень круто',
            "ТОКСИК": 'токсичный человек',
            "ТРОЛЛ ФЕЙС": 'человек который троллит всех подряд'
            
            }
            
print("Дарова старина")            
word = input("Введи непонятное слово старина (большими буквами!): ")

if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print('Сорян пупсик такого слова не нашлось')
